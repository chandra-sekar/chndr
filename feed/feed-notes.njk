---
permalink: feed/feed-notes.json
eleventyExcludeFromCollections: true
---
{
"version": "https://jsonfeed.org/version/1.1",
"title": "Chandrasekar - Front-end Developer",
"description": "The personal website of Chandrasekar",
"home_page_url": "https://chndr.cc",
"feed_url": "https://chndr.cc/feed/feed-notes.json",
"language": "en-GB",
"authors": [
  {
    "name": "Chandrasekar",
    "url": "https://chndr.cc",
    "avatar": "https://chndr.cc/img/profile-pic.png"
  }
], 
"items": [
{% for note in collections.notes %}
  {% set absolutePostUrl %}{{ note.url | url | absoluteUrl(metadata.url) }}{% endset %}
  {
    "id": {{ loop.revindex }},
    "date": "{{ note.date }}",
    "url": "{{ note.url | url | absoluteUrl(site.url) }}",
    "content": "{{ note.templateContent | safe | striptags }}",
    "content_html": "{{ note.templateContent | safe | trim }}",
    "link": "{{ absolutePostUrl }}",
    "tags": "{{ note.data.tags }}",
    "syndicate": "{{ note.data.syndicate }}"
  }
  {%- if not loop.last -%},{%- endif -%}
{% endfor %}
]
}